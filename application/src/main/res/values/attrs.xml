<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IntroView">
        <attr name="strokeWidth" format="float" />
        <attr name="strokeColor" format="color|reference" />
        <attr name="phase" format="float" />
        <attr name="duration" format="integer" />
        <attr name="fadeFactor" format="float" />
        <attr name="waitRadius" format="dimension" />
        <attr name="arrowLength" format="dimension" />
        <attr name="arrowHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="StateView">
        <attr name="strokeWidth" />
        <attr name="strokeColor" />
        <attr name="phase" />
        <attr name="duration" />
        <attr name="fadeFactor" />
    </declare-styleable>

    <declare-styleable name="ThemeViewtrip">
        <attr name="isFloatingWindow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BaseTheme">
        <attr name="actionBarIconColor" format="color" />
        <attr name="actionBarInsetStart" format="dimension" />
        <attr name="spinnerBarInsetStart" format="dimension" />
        <attr name="popupItemBackground" format="reference" />
        <attr name="photoItemForeground" format="reference" />
        <attr name="photoItemForegroundBorderless" format="reference" />
        <attr name="imageItemBackground" format="reference"/>
    </declare-styleable>
</resources>
